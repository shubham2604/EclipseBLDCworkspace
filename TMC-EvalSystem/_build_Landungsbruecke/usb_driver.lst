 252:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param handle          : USB Device handle
 253:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param endpoint_number : Endpoint number
 254:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param max_packet_size : Maximum packet size (in bytes) for the endpoint
 255:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param direction       : Direction of transfer
 256:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param endpoint_type   : Type of endpoint
 257:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param flag            : Zero termination flag
 258:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 259:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @return status
 260:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USB_OK                              : When Successfull
 261:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USBERR_EP_INIT_FAILED     : When endpoints > max Supported
 262:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  ******************************************************************************
 263:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 264:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * This function initializes an endpoint the Device layer and the Controller
 265:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * layer in the S08 USB stack. It validate whether all endpoints have already
 266:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * been initialized or not and then calls the controller layer endpoint
 267:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *  initialize function
 268:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 269:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *****************************************************************************/
 270:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** uint_8 _usb_device_init_endpoint (
 271:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     _usb_device_handle    handle,             /* [IN] USB Device handle */
 272:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                endpoint_number,    /* [IN] Endpoint number*/
 273:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_16               max_packet_size,    /* [IN] Maximum packet size (in bytes) for the endpoi
 274:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                direction,          /* [IN] Direction of transfer */
 275:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                endpoint_type,      /* [IN] Type of endpoint */
 276:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                flag                /* [IN] Zero termination flag */
 277:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** )
 278:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** {
 279:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 	USB_EP_STRUCT ep_str;
 280:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 281:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8 status = USB_OK;
 282:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 283:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* check if all endpoint have already been initialised */
 284:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     if((g_EPn == 0) && (endpoint_number != CONTROL_ENDPOINT))
 285:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 286:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         return USBERR_EP_INIT_FAILED;
 287:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 288:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 289:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     ep_str.direction = direction;
 290:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     ep_str.ep_num = endpoint_number;
 291:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 292:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #if 0 /* << EST */
 293:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #ifndef _MC9S08JS16_H
 294:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     ep_str.size = max_packet_size;
 295:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #else
 296:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     ep_str.size = (char)max_packet_size;
 297:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #endif
 298:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #else
 299:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** /* device is Kinetis K20D72 << EST */
 300:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     ep_str.size = max_packet_size;
 301:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #endif
 302:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 303:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     ep_str.type = endpoint_type;
 304:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 305:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* call controller layer for initiazation */
 306:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     status = USB_DCI_Init_EndPoint(*((uint_8*)handle), &ep_str, flag);
 307:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 308:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* if endpoint successfully initialised update counter */
 309:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     if((ep_str.ep_num != CONTROL_ENDPOINT) && (status == USB_OK))
 310:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 311:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         g_EPn--;
 312:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 313:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 314:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     return status;
 315:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** }

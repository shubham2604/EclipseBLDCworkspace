 180:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @name  _usb_device_init
 181:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 182:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @brief The function initializes the Device and Controller layer
 183:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 184:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param device_number       : USB Device controller to initialize
 185:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param handle              : pointer to USB Device handle
 186:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param number_of_endpoints : Endpoint count of the application
 187:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 188:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @return status
 189:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USB_OK                              : When Successfull
 190:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USBERR_INVALID_NUM_OF_ENDPOINTS     : When endpoints > max Supported
 191:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  ******************************************************************************
 192:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * This function initializes the Device layer and the Controller layer of the
 193:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * S08 USB stack. It initializes the variables used for this layer and then
 194:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * calls the controller layer initialize function
 195:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *****************************************************************************/
 196:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** uint_8 _usb_device_init (
 197:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8    device_number,			/* [IN] USB Device controller to initialize */
 198:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     _usb_device_handle _PTR_  handle,	/* [IN] Pointer to USB Device handle */
 199:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8    number_of_endpoints,		/* [IN] Number of endpoints to initialize */
 200:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8    bVregEn        			/* Enables or disables internal regulator */
 201:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** )
 202:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** {
 203:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 	UNUSED(handle);
 204:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 205:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* validate endpoints param */
 206:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     if((number_of_endpoints > MAX_SUPPORTED_ENDPOINTS) ||
 207:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****       (number_of_endpoints < MIN_SUPPORTED_ENDPOINTS))
 208:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 209:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         return USBERR_INVALID_NUM_OF_ENDPOINTS;
 210:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 211:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 212:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* init variables */
 213:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     g_EPn_max = (uint_8)(number_of_endpoints - 1);
 214:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 215:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     USB_Device_Init_Params();
 216:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 217:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #ifdef MULTIPLE_DEVICES
 218:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* Initialize all services to null value */
 219:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     Clear_Mem((uint_8_ptr)g_usb_CB,
 220:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         (uint_32)(sizeof(USB_SERVICE_CALLBACK) * USB_SERVICE_MAX), (uint_8)0);
 221:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #endif
 222:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* Call controller layer initialization function */
 223:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     return USB_DCI_Init(device_number, bVregEn);
 224:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 225:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** }

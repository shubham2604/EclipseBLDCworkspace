 321:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 322:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @brief The function de-initializes the endpoint
 323:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 324:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param handle          : USB Device handle
 325:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param endpoint_number : Endpoint number
 326:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param direction       : Endpoint direction
 327:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 328:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @return status
 329:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USB_OK                              : When Successfull
 330:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USBERR_EP_DEINIT_FAILED     : When endpoints > max Supported
 331:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  ******************************************************************************
 332:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 333:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * This function deinitializes an endpoint the Device layer and the Controller
 334:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * layer in the S08 USB stack. It validate whether all endpoints have already
 335:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * been deinitialized or not and then calls the controller layer endpoint
 336:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * deinitialize function
 337:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 338:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *****************************************************************************/
 339:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** uint_8 _usb_device_deinit_endpoint (
 340:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     _usb_device_handle    handle,             /* [IN] USB Device handle */
 341:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                endpoint_number,    /* [IN] Endpoint number */
 342:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                direction           /* [IN] Direction */
 343:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** )
 344:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** {
 345:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8 status=USB_OK;
 346:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 347:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* check if all endpoint have already been initialised */
 348:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     if((g_EPn == g_EPn_max) && (endpoint_number != CONTROL_ENDPOINT))
 349:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 350:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         return USBERR_EP_DEINIT_FAILED;
 351:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 352:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 353:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* call controller layer for initiazation */
 354:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     status = USB_DCI_Deinit_EndPoint(*((uint_8*)handle), endpoint_number, direction);
 355:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 356:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* if endpoint successfully deinitialised update counter */
 357:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     if((endpoint_number != CONTROL_ENDPOINT) && (status == USB_OK))
 358:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 359:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         g_EPn++;
 360:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 361:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 362:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     return status;
 363:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** }

 368:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 369:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @brief The function retrieves various endpoint as well as USB component status
 370:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 371:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param handle        : USB Device handle
 372:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param component     : USB component
 373:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param status        : Pointer to 16 bit return value
 374:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 375:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @return status
 376:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USB_OK                : When Successfull
 377:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USBERR_BAD_STATUS     : When error
 378:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 379:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  ******************************************************************************
 380:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * This function retrieves the endpoint as well USB component status which is
 381:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * stored by calling USB_Device_Set_Status. This function can be called by Ap-
 382:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * plication as well as the DCI layer.
 383:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *****************************************************************************/
 384:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** uint_8 _usb_device_get_status (
 385:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     _usb_device_handle    handle,       /* [IN]  USB Device handle */
 386:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                component,    /* [IN]  USB component */
 387:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8_ptr            status        /* [OUT] Pointer to 16 bit return value */
 388:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** )
 389:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** {
 390:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* get the endpoint number from component input variable */
 391:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8 ep_num = (uint_8)(component & USB_STATUS_ENDPOINT_NUMBER_MASK);
 392:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     UNUSED (handle);
 393:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 394:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     if((component <= USB_STATUS_TEST_MODE) &&
 395:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         (component >= USB_STATUS_DEVICE_STATE))
 396:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 397:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         /* Get the corresponding component status
 398:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****            -1 as components start from 1 */
 399:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         *status = g_usb_component_status[component-1];
 400:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 401:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     else if((component & USB_STATUS_ENDPOINT) &&
 402:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         (ep_num < MAX_SUPPORTED_ENDPOINTS))
 403:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 404:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         /* Get the corresponding endpoint status */
 405:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         *status = g_usb_ep_status[ep_num];
 406:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 407:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     else
 408:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 409:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         return USBERR_BAD_STATUS;
 410:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 411:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 412:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     return USB_OK;
 413:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** }
 414:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 415:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** /**************************************************************************//*!
 416:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 417:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @name  _usb_device_set_status
 418:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 419:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @brief The function saves status of endpoints as well as USB components.
 420:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 421:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param handle    : USB Device handle
 422:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param component : USB component
 423:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param setting   : Value to be set
 424:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 425:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @return status
 426:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USB_OK                : When Successfull
 427:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USBERR_BAD_STATUS     : When error
 428:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 429:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  ******************************************************************************
 430:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * This function sets the endpoint as well USB component status which can be
 431:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * retrieved by calling _usb_device_get_status. This function can be called by
 432:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * Application as well as the DCI layer.
 433:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *****************************************************************************/
 434:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** uint_8 _usb_device_set_status(
 435:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     _usb_device_handle    handle,       /* [IN] USB Device handle */
 436:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                component,    /* [IN] USB Component status to set */
 437:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                setting       /* [IN] Status to set */
 438:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** )
 439:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** {
 440:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* get the endpoint number from component input variable */
 441:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8 ep_num = (uint_8)(component & USB_STATUS_ENDPOINT_NUMBER_MASK);
 442:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     UNUSED (handle);
 443:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 444:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     if((component <= USB_STATUS_TEST_MODE) &&
 445:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         (component >= USB_STATUS_DEVICE_STATE))
 446:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 447:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         /*
 448:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****            Set the corresponding component setting
 449:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****            -1 as components start from 1
 450:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         */
 451:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         g_usb_component_status[component-1] = setting;
 452:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 453:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     else if((component & USB_STATUS_ENDPOINT) &&
 454:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         (ep_num < MAX_SUPPORTED_ENDPOINTS))
 455:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 456:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         uint_8 direction =
 457:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****             (uint_8)((component >> USB_COMPONENT_DIRECTION_SHIFT) &
 458:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****             USB_COMPONENT_DIRECTION_MASK);
 459:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         /* HALT Endpoint */
 460:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         if(setting == USB_STATUS_STALLED)
 461:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         {
 462:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         	_usb_device_stall_endpoint(handle, ep_num, direction);
 463:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         }
 464:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         else if((setting == USB_STATUS_IDLE) &&
 465:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****             (g_usb_ep_status[ep_num] == USB_STATUS_STALLED))
 466:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         {
 467:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         	_usb_device_unstall_endpoint(handle, ep_num, direction);
 468:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 469:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****             if(ep_num == CONTROL_ENDPOINT)
 470:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****             {
 471:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****                 direction = (uint_8)((direction == USB_SEND)?
 472:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****                     (USB_RECV):(USB_SEND));
 473:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****                 _usb_device_unstall_endpoint(handle, ep_num, direction);
 474:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****             }
 475:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         }
 476:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         /* Set the corresponding endpoint setting */
 477:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         g_usb_ep_status[ep_num] = setting;
 478:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 479:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     else
 480:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 481:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         return USBERR_BAD_STATUS;
 482:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 483:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     return USB_OK;
 484:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** }
 485:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 486:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** /**************************************************************************//*!
 487:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 488:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @name  _usb_device_register_service
 489:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 490:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @brief The function registers a callback function from the Application layer
 491:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 492:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param controller_ID : Controller ID
 493:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param type          : event type or endpoint number
 494:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param service       : callback function pointer
 495:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 496:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @return status
 497:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USB_OK                   : When Successfull
 498:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USBERR_ALLOC_SERVICE     : When invalid type or already registered
 499:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 500:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  ******************************************************************************
 501:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * This function registers a callback function from the application if it is
 502:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * called not already registered so that the registered callback function can
 503:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * be if the event of that type occurs
 504:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *****************************************************************************/
 505:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** uint_8 _usb_device_register_service(
 506:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                    controller_ID, /* [IN] Controller ID           */
 507:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                    type,          /* [IN] type of event or endpoint
 508:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****                                                      number to service       */
 509:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     USB_SERVICE_CALLBACK      service        /* [IN] pointer to callback
 510:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****                                                      function                */
 511:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** )
 512:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** {
 513:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     UNUSED (controller_ID);
 514:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     UNUSED (service);
 515:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #ifdef MULTIPLE_DEVICES
 516:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* check if the type is valid and callback for the type
 517:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****        is not already registered */
 518:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     if(((type  <= USB_SERVICE_MAX_EP) ||
 519:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         ((type < USB_SERVICE_MAX) && (type >= USB_SERVICE_BUS_RESET))) &&
 520:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****        (g_usb_CB[type] == NULL))
 521:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 522:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 523:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         /* register the callback function */
 524:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         g_usb_CB[type] = service;
 525:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         return USB_OK;
 526:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 527:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     else
 528:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 529:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         return USBERR_ALLOC_SERVICE;
 530:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 531:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #else
 532:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     UNUSED(type);
 533:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     return USB_OK;
 534:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #endif
 535:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 536:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** }
 537:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 538:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** /**************************************************************************//*!
 539:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 540:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @name  _usb_device_unregister_service
 541:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 542:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @brief The function unregisters an event or endpoint callback function
 543:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 544:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param handle         : USB Device handle
 545:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param event_endpoint : event type or endpoint number
 546:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 547:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @return status
 548:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USB_OK                   : When Successfull
 549:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USBERR_UNKNOWN_ERROR     : When invalid type or not registered
 550:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 551:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *****************************************************************************
 552:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * This function un registers a callback function which has been previously
 553:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * registered by the application layer
 554:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *****************************************************************************/
 555:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** uint_8 _usb_device_unregister_service(
 556:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     _usb_device_handle    handle,           /* [IN] USB Device handle */
 557:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                event_endpoint    /* [IN] Endpoint (0 through 15) or event to service */
 558:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** )
 559:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** {
 560:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     UNUSED (handle);
 561:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* check if the type is valid and callback for the type
 562:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****        is already registered */
 563:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     if(((event_endpoint  <= USB_SERVICE_MAX_EP) ||
 564:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         ((event_endpoint < USB_SERVICE_MAX) && (event_endpoint >= USB_SERVICE_BUS_RESET))) &&
 565:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****        (g_usb_CB[event_endpoint] != NULL))
 566:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 567:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #ifdef MULTIPLE_DEVICES
 568:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         /* unregister the callback */
 569:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         g_usb_CB[event_endpoint] = NULL;
 570:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** #endif
 571:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         return USB_OK;
 572:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 573:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     else
 574:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 575:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         return USBERR_UNKNOWN_ERROR;
 576:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 577:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** }
 578:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 579:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** /**************************************************************************//*!
 580:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 581:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @name  USB_Device_Call_Service
 582:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 583:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @brief The function is a device layer event handler
 584:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 585:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param type  : Type of service or endpoint
 586:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @param event : Pointer to event structure
 587:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 588:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * @return status
 589:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *         USB_OK      : Always
 590:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 591:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *****************************************************************************
 592:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 593:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * This function calls the registered service callback function of the applic-
 594:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * ation layer based on the type of event received. This function is called
 595:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  * from the DCI layer.
 596:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *
 597:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****  *****************************************************************************/
 598:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** uint_8 USB_Device_Call_Service(
 599:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     uint_8                    type,    /* [IN] Type of service or endpoint */
 600:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     PTR_USB_DEV_EVENT_STRUCT  event    /* [IN] Pointer to event structure  */
 601:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** )
 602:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** {
 603:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 604:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     if(type == USB_SERVICE_BUS_RESET)
 605:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {    /* if it is an reset interrupt then reset all status structures */
 606:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****        USB_Device_Init_Params();
 607:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     }
 608:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c **** 
 609:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     /* check if the callback is registered or not */
 610:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     if(g_usb_CB[type] != NULL)
 611:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****     {
 612:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         /* call the callback function */
 613:hal/Landungsbruecke/freescale/USB_CDC/usb_driver.c ****         g_usb_CB[type](event);

 180:TMC-API/tmc/helpers/CRC.c **** // Helper functions
 181:TMC-API/tmc/helpers/CRC.c **** static uint8_t flipByte(uint8_t value)
 182:TMC-API/tmc/helpers/CRC.c **** {
 183:TMC-API/tmc/helpers/CRC.c **** 	// swap odd and even bits
 184:TMC-API/tmc/helpers/CRC.c **** 	value = ((value >> 1) & 0x55) | ((value & 0x55) << 1);
 185:TMC-API/tmc/helpers/CRC.c **** 	// swap consecutive pairs
 186:TMC-API/tmc/helpers/CRC.c **** 	value = ((value >> 2) & 0x33) | ((value & 0x33) << 2);
 187:TMC-API/tmc/helpers/CRC.c **** 	// swap nibbles ...
 188:TMC-API/tmc/helpers/CRC.c **** 	value = ((value >> 4) & 0x0F) | ((value & 0x0F) << 4);
 189:TMC-API/tmc/helpers/CRC.c **** 
 190:TMC-API/tmc/helpers/CRC.c **** 	return value;
 191:TMC-API/tmc/helpers/CRC.c **** }

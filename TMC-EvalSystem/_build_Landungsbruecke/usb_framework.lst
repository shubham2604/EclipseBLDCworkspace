 859:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** static uint_8 USB_Strd_Req_Set_Address (
 860:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8    controller_ID,            /* [IN] Controller ID */
 861:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     USB_SETUP_STRUCT * setup_packet,    /* [IN] Setup packet received */
 862:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8_ptr *data,                   /* [OUT] Data to be send back */

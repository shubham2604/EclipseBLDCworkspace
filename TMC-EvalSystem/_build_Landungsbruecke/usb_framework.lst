 550:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *
 551:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @brief The function is used to send a response to the Host based.
 552:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *
 553:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param controller_ID : Controller ID
 554:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param status        : Status of request
 555:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param setup_packet  : Setup packet received
 556:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param data          : Data to be send back
 557:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param size          : Size to be returned
 558:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *
 559:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @return None
 560:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *
 561:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  ******************************************************************************
 562:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * This function sends a response to the data received on the control endpoint.
 563:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * the request is decoded in the control service
 564:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *****************************************************************************/
 565:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** static void USB_Control_Service_Handler (
 566:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8    controller_ID,            /* [IN] Controller ID */
 567:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8 status,                      /* [IN] Device Status */
 568:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     USB_SETUP_STRUCT * setup_packet,    /* [IN] Setup packet received */
 569:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8_ptr *data,                   /* [OUT] Data to be send back */
 570:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     USB_PACKET_SIZE *size               /* [OUT] Size to be returned */
 571:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** )
 572:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** {
 573:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     if(status == USBERR_INVALID_REQ_TYPE)
 574:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     {
 575:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         /* incase of error Stall endpoint */
 576:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         (void)_usb_device_set_status(&controller_ID,
 577:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             (uint_8)(USB_STATUS_ENDPOINT | CONTROL_ENDPOINT |
 578:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             (USB_SEND << USB_COMPONENT_DIRECTION_SHIFT)),
 579:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             USB_STATUS_STALLED);
 580:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     }
 581:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     else /* Need to send Data to the USB Host */
 582:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     {
 583:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         /* send the data prepared by the handlers.*/
 584:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         if(*size > setup_packet->length)
 585:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {
 586:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             *size = (USB_PACKET_SIZE)setup_packet->length;
 587:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 588:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 589:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****          /* send the data to the host */
 590:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         (void)USB_Class_Send_Data(controller_ID,
 591:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             CONTROL_ENDPOINT, *data, *size);
 592:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         if((setup_packet->request_type & USB_DATA_DIREC_MASK) ==
 593:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             USB_DATA_TO_HOST)
 594:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {   /* Request was to Get Data from device */
 595:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             /* setup rcv to get status from host */
 596:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             (void)_usb_device_recv_data(&controller_ID,
 597:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 CONTROL_ENDPOINT,NULL,0);
 598:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 599:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 600:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     }
 601:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     return;
 602:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** }

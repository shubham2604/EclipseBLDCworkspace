 720:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *
 721:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @name  USB_Strd_Req_Feature
 722:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *
 723:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @brief  This function is called in response to Clear or Set Feature request
 724:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *
 725:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param controller_ID : Controller ID
 726:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param setup_packet  : Setup packet received
 727:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param data          : Data to be send back
 728:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param size          : Size to be returned
 729:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *
 730:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @return status:
 731:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *               USB_OK : When Successfull
 732:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *               Others : When Error
 733:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *
 734:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  ******************************************************************************
 735:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * This is a ch9 request, used to set/clear a feature on the device
 736:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * (device_remote_wakeup and test_mode) or on the endpoint(ep halt)
 737:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *****************************************************************************/
 738:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** static uint_8 USB_Strd_Req_Feature (
 739:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8    controller_ID,            /* [IN] Controller ID */
 740:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     USB_SETUP_STRUCT * setup_packet,    /* [IN] Setup packet received */
 741:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8_ptr *data,                   /* [OUT] Data to be send back */
 742:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     USB_PACKET_SIZE *size               /* [OUT] Size to be returned */
 743:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** )
 744:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** {
 745:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8 device_status;
 746:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8  set_request;
 747:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8  status = USBERR_INVALID_REQ_TYPE;
 748:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8  epinfo;
 749:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8  event;
 750:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 751:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     UNUSED (data);
 752:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 753:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     *size=0;
 754:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     /* Find whether its a clear feature request or a set feature request */
 755:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     set_request = (uint_8)((setup_packet->request & USB_SET_REQUEST_MASK) >> 1 );
 756:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 757:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     if((setup_packet->request_type & USB_REQUEST_SRC_MASK) == USB_REQUEST_SRC_DEVICE)
 758:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     {
 759:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     	/* Request for Device */
 760:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 		if(set_request == TRUE)
 761:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {
 762:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             /* Standard Feature Selector */
 763:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			if((setup_packet->value == DEVICE_FEATURE_REMOTE_WAKEUP) || (setup_packet->value == DEVICE_FEATU
 764:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             {
 765:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 				status = _usb_device_get_status(&controller_ID, USB_STATUS_DEVICE, &device_status);
 766:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 767:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 				if(status == USB_OK)
 768:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 				{
 769:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 					/* Add the request to be set to the device_status */
 770:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 					device_status |= (uint_16)(1 << (uint_8)setup_packet->value);
 771:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 772:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 					/* Set the status on the device */
 773:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 					status = _usb_device_set_status(&controller_ID, USB_STATUS_DEVICE, device_status);
 774:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 				}
 775:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             }
 776:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             #ifdef OTG_BUILD
 777:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             /* OTG Feature Selector */
 778:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			else if(setup_packet->value == DEVICE_SET_FEATURE_B_HNP_ENABLE)
 779:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             {
 780:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  			   OTG_DESCRIPTOR_PTR_T otg_desc_ptr;
 781:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  			   USB_PACKET_SIZE size;
 782:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 783:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  			   status = USB_Desc_Get_Descriptor(controller_ID, USB_OTG_DESCRIPTOR,(uint_8)UNINITIALISED_VAL
 784:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  					                            (uint_16)UNINITIALISED_VAL,(uint_8_ptr *)&otg_desc_ptr,&size);
 785:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  			   if(status == USB_OK)
 786:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  			   {
 787:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  				   if(otg_desc_ptr->bmAttributes & USB_OTG_HNP_SUPPORT)
 788:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  				   {
 789:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  					  _usb_otg_hnp_enable(controller_ID, set_request);
 790:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  				   }
 791:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  			   }
 792:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             }
 793:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             #endif
 794:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             else
 795:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             {
 796:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                status = USBERR_INVALID_REQ_TYPE;
 797:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             }
 798:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 799:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         else //(set_request == FALSE) it is a clear feature request
 800:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {
 801:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         	if(setup_packet->value == DEVICE_FEATURE_REMOTE_WAKEUP)
 802:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             {
 803:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         		status = _usb_device_get_status(&controller_ID, USB_STATUS_DEVICE, &device_status);
 804:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 805:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 if(status == USB_OK)
 806:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 {
 807:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 	/* Remove the request to be cleared from device_status */
 808:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 	device_status &= (uint_16)~(1 << (uint_8)setup_packet->value);
 809:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 	status = _usb_device_set_status(&controller_ID, USB_STATUS_DEVICE, device_status);
 810:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 }
 811:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             }
 812:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         	else
 813:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         	{
 814:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         		status = USBERR_INVALID_REQ_TYPE;
 815:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         	}
 816:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 817:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     }
 818:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     else if((setup_packet->request_type & USB_REQUEST_SRC_MASK) == USB_REQUEST_SRC_ENDPOINT)
 819:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     {
 820:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         /* Request for Endpoint */
 821:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         epinfo = (uint_8)(setup_packet->index & 0x00FF);
 822:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 	    status = _usb_device_set_status(&controller_ID, (uint_8)(epinfo|USB_STATUS_ENDPOINT), set_requ
 823:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 824:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 	    event = (uint_8)(set_request ? USB_APP_EP_STALLED : USB_APP_EP_UNSTALLED);
 825:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 826:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 	    if(setup_packet->request == CLEAR_FEATURE)
 827:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {
 828:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 	         uint_8  epnum = epinfo & 0x0F;
 829:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 	         uint_8  dir = (epinfo & 0x80) >> 7;
 830:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 	         _usb_device_clear_data0_endpoint(epnum,dir);
 831:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 	         event = USB_APP_EP_UNSTALLED;  // as far as the application is concerned, this is an UNST
 832:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 833:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 834:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 	    /* Inform the upper layers of stall/unstall */
 835:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         g_framework_callback(controller_ID,event,(void*)&epinfo);
 836:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     }
 837:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     return status;
 838:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** }
 839:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 

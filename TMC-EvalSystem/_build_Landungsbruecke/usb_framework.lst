 893:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** static uint_8 USB_Strd_Req_Assign_Address (
 894:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8    controller_ID         /* [IN] Controller ID */
 895:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** )
 896:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** {
 897:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     /* Set Device Address */
 898:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     (void)_usb_device_set_address(&controller_ID, g_assigned_address);
 899:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 900:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     /* Set Device state */
 901:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     (void)_usb_device_set_status(&controller_ID,
 902:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         USB_STATUS_DEVICE_STATE, USB_STATE_ADDRESS);
 903:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     /* Set Device state */
 904:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     (void)_usb_device_set_status(&controller_ID, USB_STATUS_ADDRESS,

 464:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         /* if the request is standard request */
 465:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         if((g_setup_pkt.request_type & USB_REQUEST_CLASS_MASK) ==
 466:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             USB_REQUEST_CLASS_STRD)
 467:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {
 468:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             /* if callback is not NULL */
 469:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             if(g_standard_request[g_setup_pkt.request] != NULL)
 470:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             {
 471:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 /* if the request is valid in this device state */
 472:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 if((device_state < USB_STATE_POWERED) &&
 473:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                     (g_validate_request[g_setup_pkt.request][device_state]
 474:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                     ==TRUE))
 475:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 {
 476:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                     /* Standard Request function pointers */
 477:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                     status = g_standard_request[g_setup_pkt.request]
 478:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                         (event->controller_ID, &g_setup_pkt, &data,
 479:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                         (USB_PACKET_SIZE *)&size);
 480:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 }
 481:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             }
 482:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 483:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         else /* for Class/Vendor requests */
 484:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {
 485:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             /*get the length from the setup_request*/
 486:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             size = (USB_PACKET_SIZE)g_setup_pkt.length;
 487:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             if(size <= USB_OUT_PKT_SIZE) {
 488:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             if( (size != 0) &&
 489:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 ((g_setup_pkt.request_type & USB_DATA_DIREC_MASK) ==
 490:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 USB_DATA_TO_DEVICE) )
 491:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             {
 492:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 (void)memcpy(&ext_req_to_host_setup, &g_setup_pkt, USB_SETUP_PKT_SIZE);
 493:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 494:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 /* expecting host to send data (OUT TRANSACTION)*/
 495:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 (void)_usb_device_recv_data(&(event->controller_ID),
 496:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                     CONTROL_ENDPOINT, ext_req_to_host_buffer,
 497:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                     (USB_PACKET_SIZE)(size));
 498:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 return;
 499:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             }
 500:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             else if(g_other_req_callback != NULL)/*call class/vendor request*/
 501:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             {
 502:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 status = g_other_req_callback(event->controller_ID,
 503:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                     &g_setup_pkt, &data, (USB_PACKET_SIZE*)&size);
 504:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             }
 505:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 506:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             else
 507:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             {
 508:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 /* incase of error Stall endpoint */
 509:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 USB_Control_Service_Handler(event->controller_ID,
 510:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 	USBERR_INVALID_REQ_TYPE, &g_setup_pkt, &data, (USB_PACKET_SIZE*)&size);
 511:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             	return;
 512:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             }
 513:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 514:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 515:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         USB_Control_Service_Handler(event->controller_ID,
 516:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             status, &g_setup_pkt, &data, (USB_PACKET_SIZE*)&size);
 517:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     }
 518:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     /* if its not a setup request */
 519:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     else if(device_state == USB_STATE_PENDING_ADDRESS)
 520:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     {
 521:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         /* Device state is PENDING_ADDRESS */
 522:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         /* Assign the new adddress to the Device */
 523:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         (void)USB_Strd_Req_Assign_Address(event->controller_ID);
 524:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         return;
 525:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     }
 526:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     else if( ((g_setup_pkt.request_type &
 527:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             USB_DATA_DIREC_MASK) == USB_DATA_TO_DEVICE) &&
 528:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             (event->direction == USB_RECV) )
 529:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     {
 530:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         /* execution enters Control Service because of
 531:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****            OUT transaction on CONTROL_ENDPOINT*/
 532:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         if(g_other_req_callback != NULL)
 533:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {   /* class or vendor request */
 534:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             size = (USB_PACKET_SIZE)(event->len + USB_SETUP_PKT_SIZE);
 535:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             status = g_other_req_callback(event->controller_ID,
 536:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 &ext_req_to_host_setup, &data,
 537:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****                 &size);
 538:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 539:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 540:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         USB_Control_Service_Handler(event->controller_ID,
 541:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             status, &g_setup_pkt, &data,
 542:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****             (USB_PACKET_SIZE*)&size);
 543:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     }
 544:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     return;

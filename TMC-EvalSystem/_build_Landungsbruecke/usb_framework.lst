 609:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param controller_ID : Controller ID
 610:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param setup_packet  : Setup packet received
 611:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param data          : Data to be send back
 612:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @param size          : Size to be returned
 613:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *
 614:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * @return status:
 615:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *               USB_OK : When Successfull
 616:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *               Others : When Error
 617:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *
 618:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  ******************************************************************************
 619:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * This is a ch9 request and is called by the host to know the status of the
 620:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  * device, the interface and the endpoint
 621:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****  *****************************************************************************/
 622:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** static uint_8 USB_Strd_Req_Get_Status (
 623:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8    controller_ID,            /* [IN] Controller ID */
 624:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     USB_SETUP_STRUCT * setup_packet,    /* [IN] Setup packet received */
 625:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8_ptr *data,                   /* [OUT] Data to be send back */
 626:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     USB_PACKET_SIZE *size               /* [OUT] Size to be returned */
 627:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** )
 628:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** {
 629:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 	uint_8_ptr pu8ConfigDesc = NULL;
 630:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 	uint_16 u16DescSize;
 631:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8 interface, endpoint = 0;
 632:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8 status;
 633:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     uint_8 device_state = USB_STATE_POWERED;
 634:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 635:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 636:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     status = _usb_device_get_status(&controller_ID, USB_STATUS_DEVICE_STATE, &device_state);
 637:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 638:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     if((setup_packet->request_type & USB_REQUEST_SRC_MASK) == USB_REQUEST_SRC_DEVICE)
 639:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     {
 640:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         #ifdef OTG_BUILD
 641:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         if(setup_packet->index == USB_WINDEX_OTG_STATUS_SEL)
 642:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {
 643:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			uint_8 temp;
 644:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			 /* Request for Device */
 645:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			 status = _usb_device_get_status(&controller_ID, USB_STATUS_OTG, &temp);
 646:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			 g_std_framework_data = (uint_16)temp;
 647:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			 g_std_framework_data &= GET_STATUS_OTG_MASK;
 648:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			 g_std_framework_data = BYTE_SWAP16(g_std_framework_data);
 649:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			 *size = OTG_STATUS_SIZE;
 650:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 651:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         else
 652:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         #endif
 653:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {
 654:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****          uint_8 device_state1;
 655:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 656:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****          /* Request for Device */
 657:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****          status = _usb_device_get_status(&controller_ID, USB_STATUS_DEVICE, &device_state1);
 658:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****          g_std_framework_data = (uint_16)device_state1;
 659:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****          g_std_framework_data &= GET_STATUS_DEVICE_MASK;
 660:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****          g_std_framework_data = BYTE_SWAP16(g_std_framework_data);
 661:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 662:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****          /* Set Self Powered bit in device state according to configuration */
 663:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****          status = USB_Desc_Get_Descriptor(controller_ID, USB_CONFIG_DESCRIPTOR,
 664:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         		                          0, 0, &pu8ConfigDesc, (USB_PACKET_SIZE *)&u16DescSize);
 665:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 666:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 		 if((pu8ConfigDesc[7] & SELF_POWERED) != 0)
 667:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 		 {
 668:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			g_std_framework_data |= BYTE_SWAP16(0x0001);
 669:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 		 }
 670:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 		 else
 671:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 		 {
 672:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			g_std_framework_data &= BYTE_SWAP16(~0x0001);
 673:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 		 }
 674:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****          *size = DEVICE_STATUS_SIZE;
 675:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 676:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     }
 677:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     else if((setup_packet->request_type & USB_REQUEST_SRC_MASK) == USB_REQUEST_SRC_INTERFACE)
 678:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     {
 679:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         /* Request for Interface */
 680:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     	interface = (uint_8) setup_packet->index;
 681:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         if((device_state == USB_STATE_ADDRESS) && (interface > 0))
 682:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {
 683:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         	 status = USBERR_INVALID_REQ_TYPE;
 684:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 685:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         else
 686:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         {
 687:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         	/* Interface requests always return 0 */
 688:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         	g_std_framework_data = 0x0000;
 689:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			*size = INTERFACE_STATUS_SIZE;
 690:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         }
 691:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     }
 692:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     else if((setup_packet->request_type & USB_REQUEST_SRC_MASK) == USB_REQUEST_SRC_ENDPOINT)
 693:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     {
 694:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****         /* Request for Endpoint */
 695:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     	if((device_state == USB_STATE_ADDRESS) && (endpoint > 0))
 696:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 		{
 697:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			 status = USBERR_INVALID_REQ_TYPE;
 698:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 		}
 699:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     	else
 700:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     	{
 701:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			uint_8 device_state2;
 702:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			endpoint  = (uint_8)(((uint_8) setup_packet->index) |
 703:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 				USB_STATUS_ENDPOINT);
 704:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			status =  _usb_device_get_status(&controller_ID,
 705:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 				endpoint,
 706:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 				&device_state2);
 707:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			g_std_framework_data = (uint_16)device_state2;
 708:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			/* All other fields except HALT must be zero */
 709:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			g_std_framework_data &= 0x0001;  /* LSB is for the HALT feature */
 710:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			g_std_framework_data = BYTE_SWAP16(g_std_framework_data);
 711:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 			*size = ENDP_STATUS_SIZE;
 712:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     	}
 713:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     }
 714:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** 
 715:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     *data = (uint_8_ptr)&g_std_framework_data;
 716:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c ****     return status;
 717:hal/Landungsbruecke/freescale/USB_CDC/usb_framework.c **** }

  53:hal/Landungsbruecke/tmc/RS232.c **** static void init()
  54:hal/Landungsbruecke/tmc/RS232.c **** {
  55:hal/Landungsbruecke/tmc/RS232.c **** 	register uint16_t ubd;
  56:hal/Landungsbruecke/tmc/RS232.c **** 
  57:hal/Landungsbruecke/tmc/RS232.c **** 	SIM_SCGC1 |= (SIM_SCGC1_UART4_MASK);
  58:hal/Landungsbruecke/tmc/RS232.c **** 
  59:hal/Landungsbruecke/tmc/RS232.c **** 	HAL.IOs->pins->RS232_RX.configuration.GPIO_Mode = GPIO_Mode_AF3;
  60:hal/Landungsbruecke/tmc/RS232.c **** 	HAL.IOs->pins->RS232_TX.configuration.GPIO_Mode = GPIO_Mode_AF3;
  61:hal/Landungsbruecke/tmc/RS232.c **** 
  62:hal/Landungsbruecke/tmc/RS232.c **** 	HAL.IOs->config->set(&HAL.IOs->pins->RS232_RX);
  63:hal/Landungsbruecke/tmc/RS232.c **** 	HAL.IOs->config->set(&HAL.IOs->pins->RS232_TX);
  64:hal/Landungsbruecke/tmc/RS232.c **** 	/* Disable the transmitter and receiver */
  65:hal/Landungsbruecke/tmc/RS232.c **** 	UART_C2_REG(UART4_BASE_PTR) &= ~(UART_C2_TE_MASK | UART_C2_RE_MASK);
  66:hal/Landungsbruecke/tmc/RS232.c **** 
  67:hal/Landungsbruecke/tmc/RS232.c **** 	/* Configure the UART for 8-bit mode, no parity */
  68:hal/Landungsbruecke/tmc/RS232.c **** 	/* We need all default settings, so entire register is cleared */
  69:hal/Landungsbruecke/tmc/RS232.c **** 	UART_C1_REG(UART4_BASE_PTR) = 0;
  70:hal/Landungsbruecke/tmc/RS232.c **** 
  71:hal/Landungsbruecke/tmc/RS232.c **** 	ubd = (CPU_BUS_CLK_HZ / 16) / (RS232.baudRate);
  72:hal/Landungsbruecke/tmc/RS232.c **** 
  73:hal/Landungsbruecke/tmc/RS232.c **** 	UART_BDH_REG(UART4_BASE_PTR) = (ubd >> 8) & UART_BDH_SBR_MASK;
  74:hal/Landungsbruecke/tmc/RS232.c **** 	UART_BDL_REG(UART4_BASE_PTR) = (ubd & UART_BDL_SBR_MASK);
  75:hal/Landungsbruecke/tmc/RS232.c **** 
  76:hal/Landungsbruecke/tmc/RS232.c **** 	/* Enable receiver and transmitter */
  77:hal/Landungsbruecke/tmc/RS232.c **** 	UART_C2_REG(UART4_BASE_PTR) |= (UART_C2_TE_MASK | UART_C2_RE_MASK | UART_C2_RIE_MASK);
  78:hal/Landungsbruecke/tmc/RS232.c **** 
  79:hal/Landungsbruecke/tmc/RS232.c **** 	enable_irq(INT_UART4_RX_TX-16);
  80:hal/Landungsbruecke/tmc/RS232.c **** }

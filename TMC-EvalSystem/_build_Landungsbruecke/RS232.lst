  97:hal/Landungsbruecke/tmc/RS232.c **** void UART4_RX_TX_IRQHandler(void)
  98:hal/Landungsbruecke/tmc/RS232.c **** {
  99:hal/Landungsbruecke/tmc/RS232.c **** 	uint32_t status = UART4_S1;
 100:hal/Landungsbruecke/tmc/RS232.c **** 
 101:hal/Landungsbruecke/tmc/RS232.c **** 	if(status & UART_S1_RDRF_MASK)
 102:hal/Landungsbruecke/tmc/RS232.c **** 	{
 103:hal/Landungsbruecke/tmc/RS232.c **** 		buffers.rx.buffer[buffers.rx.wrote] = UART4_D;
 104:hal/Landungsbruecke/tmc/RS232.c **** 		buffers.rx.wrote = (buffers.rx.wrote + 1) % BUFFER_SIZE;
 105:hal/Landungsbruecke/tmc/RS232.c **** 		available++;
 106:hal/Landungsbruecke/tmc/RS232.c **** 
 107:hal/Landungsbruecke/tmc/RS232.c **** 		UART4_S1 &= ~(UART_S1_RDRF_MASK);
 108:hal/Landungsbruecke/tmc/RS232.c **** 	}
 109:hal/Landungsbruecke/tmc/RS232.c **** 
 110:hal/Landungsbruecke/tmc/RS232.c **** 	if(status & UART_S1_TDRE_MASK)
 111:hal/Landungsbruecke/tmc/RS232.c **** 	{
 112:hal/Landungsbruecke/tmc/RS232.c **** 		if(buffers.tx.read != buffers.tx.wrote)
 113:hal/Landungsbruecke/tmc/RS232.c **** 		{
 114:hal/Landungsbruecke/tmc/RS232.c **** 			UART4_D	= buffers.tx.buffer[buffers.tx.read];
 115:hal/Landungsbruecke/tmc/RS232.c **** 			buffers.tx.read = (buffers.tx.read + 1) % BUFFER_SIZE;
 116:hal/Landungsbruecke/tmc/RS232.c **** 		}
 117:hal/Landungsbruecke/tmc/RS232.c **** 		else
 118:hal/Landungsbruecke/tmc/RS232.c **** 		{ // empty buffer -> turn off send interrupt
 119:hal/Landungsbruecke/tmc/RS232.c **** 			UART4_C2 &= ~UART_C2_TIE_MASK;
 120:hal/Landungsbruecke/tmc/RS232.c **** 		}
 121:hal/Landungsbruecke/tmc/RS232.c **** 		UART4_S1 &= ~(UART_S1_TDRE_MASK);
 122:hal/Landungsbruecke/tmc/RS232.c **** 	}
 123:hal/Landungsbruecke/tmc/RS232.c **** }

  79:boards/TMC8462_eval.c **** /*
  80:boards/TMC8462_eval.c ****  * Register access types: 32-bit and 64-bit
  81:boards/TMC8462_eval.c ****  * default: 64-bit
  82:boards/TMC8462_eval.c ****  * 32-bit access on: TMC8462_MFC_SPI_RX_DATA, TMC8462_MFC_SPI_TX_DATA, TMC8462_MFC_PWM4
  83:boards/TMC8462_eval.c ****  */
  84:boards/TMC8462_eval.c **** 
  85:boards/TMC8462_eval.c **** static void register_write(uint8_t motor, uint8_t address, int32_t value)
  86:boards/TMC8462_eval.c **** {
  87:boards/TMC8462_eval.c **** 	static uint8_t write_buffer[8];
  88:boards/TMC8462_eval.c **** 
  89:boards/TMC8462_eval.c **** 	address = TMC8462_MFC(address);
  90:boards/TMC8462_eval.c **** 
  91:boards/TMC8462_eval.c **** 	switch(motor)
  92:boards/TMC8462_eval.c **** 	{
  93:boards/TMC8462_eval.c **** 	case TMC8462_MOTOR_LOW:
  94:boards/TMC8462_eval.c **** 		write_buffer[0] = BYTE(value, 0);
  95:boards/TMC8462_eval.c **** 		write_buffer[1] = BYTE(value, 1);
  96:boards/TMC8462_eval.c **** 		write_buffer[2] = BYTE(value, 2);
  97:boards/TMC8462_eval.c **** 		write_buffer[3] = BYTE(value, 3);
  98:boards/TMC8462_eval.c **** 		break;
  99:boards/TMC8462_eval.c **** 	case TMC8462_MOTOR_HIGH:
 100:boards/TMC8462_eval.c **** 		write_buffer[4] = BYTE(value, 0);
 101:boards/TMC8462_eval.c **** 		write_buffer[5] = BYTE(value, 1);
 102:boards/TMC8462_eval.c **** 		write_buffer[6] = BYTE(value, 2);
 103:boards/TMC8462_eval.c **** 		write_buffer[7] = BYTE(value, 3);
 104:boards/TMC8462_eval.c **** 		break;
 105:boards/TMC8462_eval.c **** 	default:
 106:boards/TMC8462_eval.c **** 		write_buffer[0] = BYTE(value, 0);
 107:boards/TMC8462_eval.c **** 		write_buffer[1] = BYTE(value, 1);
 108:boards/TMC8462_eval.c **** 		write_buffer[2] = BYTE(value, 2);
 109:boards/TMC8462_eval.c **** 		write_buffer[3] = BYTE(value, 3);
 110:boards/TMC8462_eval.c **** 		break;
 111:boards/TMC8462_eval.c **** 	}
 112:boards/TMC8462_eval.c **** 
 113:boards/TMC8462_eval.c **** 	tmc8462_mfc_write_auto(&tmc8462, address, write_buffer);
 114:boards/TMC8462_eval.c **** }
 115:boards/TMC8462_eval.c **** 
 116:boards/TMC8462_eval.c **** static void register_read(uint8_t motor, uint8_t address, int32_t *value)
 117:boards/TMC8462_eval.c **** {
 118:boards/TMC8462_eval.c **** 	static uint8_t readBuffer[8];
 119:boards/TMC8462_eval.c **** 
 120:boards/TMC8462_eval.c **** 	address = TMC8462_MFC(address);
 121:boards/TMC8462_eval.c **** 
 122:boards/TMC8462_eval.c **** 	tmc8462_mfc_read_auto(&tmc8462, address, readBuffer);
 123:boards/TMC8462_eval.c **** 
 124:boards/TMC8462_eval.c **** 	switch (motor)
 125:boards/TMC8462_eval.c **** 	{
 126:boards/TMC8462_eval.c **** 	case TMC8462_MOTOR_LOW:
 127:boards/TMC8462_eval.c **** 		*value = (readBuffer[3] << 24) | (readBuffer[2] << 16) | (readBuffer[1] << 8) | readBuffer[0];
 128:boards/TMC8462_eval.c **** 		break;
 129:boards/TMC8462_eval.c **** 	case TMC8462_MOTOR_HIGH:
 130:boards/TMC8462_eval.c **** 		*value = (readBuffer[7] << 24) | (readBuffer[6] << 16) | (readBuffer[5] << 8) | readBuffer[4];
 131:boards/TMC8462_eval.c **** 		break;
 132:boards/TMC8462_eval.c **** 	default:
 133:boards/TMC8462_eval.c **** 		*value = (readBuffer[3] << 24) | (readBuffer[2] << 16) | (readBuffer[1] << 8) | readBuffer[0];
 134:boards/TMC8462_eval.c **** 		break;
 135:boards/TMC8462_eval.c **** 	}
 136:boards/TMC8462_eval.c **** }
 137:boards/TMC8462_eval.c **** 

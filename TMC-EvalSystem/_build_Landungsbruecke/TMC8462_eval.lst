 206:boards/TMC8462_eval.c **** static uint32_t user_function(uint8_t type, uint8_t motor, int32_t *value)
 207:boards/TMC8462_eval.c **** {
 208:boards/TMC8462_eval.c **** 	UNUSED(motor);
 209:boards/TMC8462_eval.c **** 
 210:boards/TMC8462_eval.c **** 	uint32_t reply = TMC_ERROR_NONE;
 211:boards/TMC8462_eval.c **** 
 212:boards/TMC8462_eval.c **** 	switch(type)
 213:boards/TMC8462_eval.c **** 	{
 214:boards/TMC8462_eval.c **** 	case UF_PDI_RESET:
 215:boards/TMC8462_eval.c **** 		pdi_reset();
 216:boards/TMC8462_eval.c **** 		break;
 217:boards/TMC8462_eval.c **** 	case UF_EEP_READ:
 218:boards/TMC8462_eval.c **** 		reply = eep_read(value);
 219:boards/TMC8462_eval.c **** 		break;
 220:boards/TMC8462_eval.c **** 	case UF_EEP_WRITE:
 221:boards/TMC8462_eval.c **** 		reply = eep_write(*value);
 222:boards/TMC8462_eval.c **** 		break;
 223:boards/TMC8462_eval.c **** 	default:
 224:boards/TMC8462_eval.c **** 		reply = TMC_ERROR_TYPE;
 225:boards/TMC8462_eval.c **** 		break;
 226:boards/TMC8462_eval.c **** 	}
 227:boards/TMC8462_eval.c **** 
 228:boards/TMC8462_eval.c **** 	return reply;
 229:boards/TMC8462_eval.c **** }

  49:boards/TMC8462_eval.c **** /*
  50:boards/TMC8462_eval.c ****  * Usage note:
  51:boards/TMC8462_eval.c ****  * There are 2 possible modes to use the API for this chip.
  52:boards/TMC8462_eval.c ****  * 1. Use two different configurations for each channel and switch between ESC and MFC by using the
  53:boards/TMC8462_eval.c ****  * 2. Use a single configuration for channel 1 and use local wrapper, setting the chip select there
  54:boards/TMC8462_eval.c ****  *
  55:boards/TMC8462_eval.c ****  * Here the first method is used.
  56:boards/TMC8462_eval.c ****  */
  57:boards/TMC8462_eval.c **** uint8_t tmc8462_readWrite(uint8_t channel, uint8_t data, uint8_t lastTransfer)
  58:boards/TMC8462_eval.c **** {
  59:boards/TMC8462_eval.c **** 	uint8_t out = 0;
  60:boards/TMC8462_eval.c **** 
  61:boards/TMC8462_eval.c **** 	switch(channel)
  62:boards/TMC8462_eval.c **** 	{
  63:boards/TMC8462_eval.c **** 	case CHANNEL_1: // Channel 1 => readWrite on ESC
  64:boards/TMC8462_eval.c **** 		out = tmc8462_spi_esc->readWrite(data, lastTransfer);
  65:boards/TMC8462_eval.c **** 		break;
  66:boards/TMC8462_eval.c **** 	case CHANNEL_2: // Channel 2 => readWrite on MFC
  67:boards/TMC8462_eval.c **** 		out = tmc8462_spi_mfc->readWrite(data, lastTransfer);
  68:boards/TMC8462_eval.c **** 		break;
  69:boards/TMC8462_eval.c **** 	}
  70:boards/TMC8462_eval.c **** 
  71:boards/TMC8462_eval.c **** 	return out;
  72:boards/TMC8462_eval.c **** }
